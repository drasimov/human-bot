function [a, z, w, b] = train(a, z, y, w, b, r)
    % for i = 1:length(b)-1
    %     [a{i+1}, z{i+1}] = feedforward(w{i}, b{i}, a{i},1);
    % end
    % [a{length(b)+1}, z{length(b)+1}] = feedforward(w{length(b)}, b{length(b)}, a{length(b)},2);

    [a,z] = feedforward(w,)
    [dw, db] = backprop(a, z, y, w, b);
    
    for i = 1:length(b)
        b{i} = b{i}-r.*db{i};
        w{i} = w{i}-r.*dw{i};
    end
end